{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/assets/*", "/checkbox-img/*", "/tokens-img/*", "/*.png", "/*.js", "/*.css"]
  },
  "mimeTypes": {
    ".js": "application/javascript",
    ".css": "text/css",
    ".png": "image/png"
  },
  "globalHeaders": {
    "Content-Security-Policy": "default-src 'self' https//*.github.com;"
  }
}